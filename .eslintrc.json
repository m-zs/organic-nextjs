{
  "extends": [
    "next/core-web-vitals",
    "plugin:prettier/recommended",
    "plugin:jest/recommended",
    "plugin:testing-library/react",
    "plugin:storybook/recommended"
  ],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "@next/next/no-document-import-in-page": "off",
    "@next/next/no-page-custom-font": "off"
  }
}
